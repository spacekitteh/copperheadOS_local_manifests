<manifest>
<remote name="matrix.org"
        fetch="https://matrix.org/git/" 
        revision="master" />
<remote name="matrix-github"
        fetch="https://github.com/matrix-org" 
        revision="master" />
<remote name="riot-github" 
        fetch="https://github.com/vector-im"
        revision="master" />

